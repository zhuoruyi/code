double fact(int x)
{
	int y; double z = 1;
	if (x == 0)
		z = x;
	else
	{
		for (y = 1; y<= x; y++)
		{
			z = z * y;
		}
	}
	return z;
}
int main()
{
	int n; int i; int product;
	scanf("%d", &n);
	for (i = 0; i <= n; i++)
	{
		product = fact(i);
		printf("i!=%d\n", product);
	}

	return 0;
}
